pipeline {
    agent none
    stages {
stage('Test') {
            agent {
                docker {
                    image 'izaldal/django:team12'
                }
            }
            steps {
                dir("educationweb") {
                
                        sh 'ls'
                        sh 'python manage.py test --keepdb'
                }
            }
            
        }
 stage('Integration Test') {
            agent {
                docker {
                    image 'izaldal/django:team12'
                }
            }
            steps {
                dir("educationweb\kindergarten\tests") {
                        
                    sh 'ls'
                        sh 'python manage.py test integration_test --keepdb'
                }
            }
            
        }




    }
}
