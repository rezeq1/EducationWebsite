pipeline {
    agent none
    stages {
stage('Build') {
            agent {
                docker {
                    image 'amarhabneen/bm_team7'
                }
            }
            steps {
                sh 'cd educationweb/'
                sh 'python manage.py test --keepdb'
            }
            
        }

        stage('Test') {
            agent {
                docker {
                    image 'docker'
                }
            }
            steps {
                sh 'python educationweb/manage.py test --keepdb'
                
            }
        }
    }
}
