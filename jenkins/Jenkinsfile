pipeline {
    agent none
    stages {
stage('Build') {
            agent {
                docker {
                    image 'amarhabneen/bm_team7'
                }
            }
            steps {
                sh 'python manage.py test'
            }
            
        }

        stage('Test') {
            agent {
                docker {
                    image 'amarhabneen/bm_team7'
                }
            }
            steps {
                sh 'python manage.py test'
            }
        }
    }
}